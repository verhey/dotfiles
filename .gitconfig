[user]
    name = Dean Verhey
    email = deanverhey@gmail.com
[core]
    autocrlf = false
    editor = nano
    excludesfile = ~/.gitignore_global
[alias]
    adog = log --all --decorate --oneline --graph
    cm = checkout master
    cmn = checkout main
    cd = checkout develop
    unstage = reset HEAD --
[fetch]
    prune = true
[pull]
    ff = only
# if adding to yapta/coupa repos, use yapta/coupa info
[includeIf "gitdir:~/yapta/**"]
    path = .gitconfig_work
[includeIf "gitdir:~/coupa/**"]
    path = .gitconfig_work
